{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
             "no-console": "warn",
            "react/prop-types": "off",
             "react/self-closing-comp": "warn",
             "react/react-in-jsx-scope": "off"
            },
           "settings": {
             "react": {
               "version": "detect"
             }
           }
}
